      <nav class="nav"  aria-label="primary">
        <ul class="nav__list">
        {% set navPrimary = collections.all | eleventyNavigation("Primary") %}
        {%- for entry in navPrimary %}
          <li class="nav__list-item"><a class="nav__list-item-link"  href="{{ entry.url | url }}"  {% if entry.url == page.url %}aria-current="page"{% endif %}>{{ entry.title }}</a></li>
        {%- endfor %}
        </ul>
      </nav>
      </div>
    </header>

    {% if heroTitle %}
      <section class="hero" aria-labelledby="hero-title">
        <h2 class="hero__title" id="hero-title">{{heroTitle}}</h2>
        {% if heroDescription %}
        <p class="hero__description">{{heroDescription}}</p>
        {% endif %}
      </section>
    {% endif %}

    <main id="content" class="content">
      {{ content | safe }}
    </main>

    <footer class="footer">
      <div class="footer__inner">
        <a class="logo__link" href="{{ '/' | url }}">{{ metadata.title }}</a>

        <div class="footer__links">
          <nav aria-label="secondary" class="footer__nav">
            <ul class="footer__nav-list">
              {% set navSecondary = collections.all | eleventyNavigation("Secondary") %}
              {%- for entry in navSecondary %}
                <li class="footer__nav-item"><a class="footer__nav-link"  href="{{ entry.url | url }}" aria-current="{% if entry.url == page.url %}page{% endif %}">{{ entry.title }}</a></li>
              {%- endfor %}
            </ul>
          </nav>

          <nav aria-label="external" class="footer__nav">
            <ul class="footer__nav-list">
              <li class="footer__nav-item"><a class="footer__nav-link" target="blank" href="https://github.com/tiaanduplessis" rel="me">GitHub</a></li>
              <li class="footer__nav-item"><a class="footer__nav-link"  target="blank" href="https://www.linkedin.com/in/tiaanduplessis" rel="me">LinkedIn</a></li>
              <li class="footer__nav-item"><a class="footer__nav-link"  target="blank" href="mailto:tiaanduplessis@hotmail.com" rel="me">Email</a></li>
            </ul>
          </nav>
      </div>
      </div>
    </footer>
  </body>
  <script>
    if ('serviceWorker' in navigator) {
      window.addEventListener('load', function() {
        navigator.serviceWorker.register('/service-worker.js');
      });
    }
